using System;

class Program
{
    static void Main()
    {
        PrintEvenNumbersUpToN(5);
        PrintEvenNumbersUpToN(8);
    }

    static void PrintEvenNumbersUpToN(int N)
    {
        Console.Write("Чётные числа от 1 до " + N + " -> ");
        for (int i = 2; i <= N; i += 2)
        {
            Console.Write(i + " ");
        }
        Console.WriteLine();
    }
}
